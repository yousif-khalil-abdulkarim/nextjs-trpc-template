{
    "name": "outstanding-resumes",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "playwright:install": "npx playwright install chromium firefox webkit",
        "husky:install": "npx husky install",
        "_commit-msg": "npx commitlint --edit",
        "test:components": "npx jest --config ./jest-components.config.js",
        "test:unit": "npx jest --config ./jest-unit.config.js",
        "test:e2e": "npx playwright test --config ./playwright.config.ts",
        "test": "npm run test:unit && npm run test:components && npm run test:e2e",
        "test:coverage": "npm run test:unit -- --coverage",
        "_build:next:lint": "npx eslint --ignore-path ./.eslintignore --config ./.eslintrc.build.js \"**/*.{js,ts,jsx,tsx}\"",
        "_build:next:check": "npx tsc --project ./tsconfig.build.json --noEmit",
        "_build:next": "npx next build",
        "build:next": "npm run test && npm run _build:next:check && npm run _build:next:lint && npm run _build:next",
        "start:next": "npx next start -p 3000",
        "dev:next": "npx next dev -p 3000",
        "_build:ladle": "npx ladle build",
        "build:ladle": "npm run test:components && npm run _build:ladle",
        "start:ladle": "npx ladle preview",
        "dev:ladle": "npx ladle dev",
        "_check": "npx tsc --project ./tsconfig.json --noEmit",
        "_lint": "npx eslint --ignore-path ./.eslintignore --config ./.eslintrc.js \"**/*.{js,ts,jsx,tsx}\"",
        "lint": "npm run _lint && npm run _check",
        "_format": "npx prettier --write --ignore-path ./.prettierignore --config .prettierrc.json \"**/*.json\"",
        "_lint:fix": "npm run _lint -- --fix",
        "lint:fix": "npm run _format && npm run _lint:fix && npm run _check"
    },
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/server": "^11.10.0",
        "@mantine/core": "^5.9.5",
        "@mantine/dates": "^5.9.5",
        "@mantine/dropzone": "^5.9.5",
        "@mantine/hooks": "^5.9.5",
        "@mantine/modals": "^5.9.5",
        "@mantine/next": "^5.9.5",
        "@mantine/notifications": "^5.9.5",
        "@mantine/tiptap": "^5.9.5",
        "@tabler/icons": "^1.118.0",
        "@tanstack/react-query": "^4.20.4",
        "@tiptap/extension-link": "^2.0.0-beta.209",
        "@tiptap/react": "^2.0.0-beta.209",
        "@tiptap/starter-kit": "^2.0.0-beta.209",
        "@trpc/client": "^10.7.0",
        "@trpc/next": "^10.7.0",
        "@trpc/react-query": "^10.7.0",
        "@trpc/server": "^10.7.0",
        "dayjs": "^1.11.7",
        "next": "13.1.1",
        "next-auth": "^4.18.7",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "superjson": "^1.12.1",
        "zod": "^3.20.2"
